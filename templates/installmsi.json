    "installer": {
        "script": "Invoke-ExternalCommand msiexec -ArgumentList @('/i', \"$dir\\setup.msi_\", '/qn') -RunAs | Out-Null"
    },
    "post_install": [
        "$CommonDesktopPath = [Environment]::GetFolderPath(\"CommonDesktopDirectory\")",
        "if (Test-Path \"$CommonDesktopPath\\Program.lnk\") {Remove-Item \"$CommonDesktopPath\\Program.lnk\" -Force}"
    ],
    "uninstaller": {
        "script": [
            "$id = wmic product where \"\"\"name like 'Program Name%'\"\"\" get IdentifyingNumber | find /i \"\"\"-\"\"\"",
            "Start-Process -Wait -WindowStyle Hidden cmd \"/c msiexec /x $id /qn /norestart\" -Verb RunAs"
        ]
    },
    "checkver": {
        "url": "https://website",
        "regex": "(starttext[\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://website/filename.msi#/setup.msi_"
            },
