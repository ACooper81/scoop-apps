{
    "version": "10.10.0",
    "description": "Jellyfin Server. Software Media System",
    "homepage": "https://jellyfin.org",
    "license": "GPL-2.0-only",
    "suggest": {
        "ffmpeg": [
            "ffmpeg",
            "ffmpeg-nightly"
        ],
        "nssm": "nssm"
    },
    "architecture": {
        "64bit": {
            "url": "https://repo.jellyfin.org/files/server/windows/latest-stable/amd64/jellyfin_10.10.0-amd64.zip",
            "hash": "0de90e69b5dd7d935d807673eaa734a1bf79659a01853b90f8bc8dc911f9a878"
        }
    },
    "extract_dir": "jellyfin_10.10.0",
    "extract_to": "system",
    "bin": [
        [
            "system\\jellyfin.exe",
            "jellyfin",
            "-d \"$dir\\data\""
        ]
    ],
    "persist": "data",
    "checkver": {
        "url": "https://repo.jellyfin.org/?path=/server/windows/latest-stable/amd64",
        "regex": "Current Stable \\(v([\\d+.]+)\\)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://repo.jellyfin.org/files/server/windows/latest-stable/amd64/jellyfin_$version-amd64.zip"
            }
        },
        "hash": {
            "url": "$url.sha256sum"
        },
        "extract_dir": "jellyfin_$version"
    }
}
