{
    "version": "5.0.10",
    "description": "The .NET Desktop Runtime enables you to run existing Windows desktop applications.",
    "homepage": "https://dotnet.microsoft.com/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/5.0.10/windowsdesktop-runtime-5.0.10-win-x64.exe#/setup.exe",
            "hash": "sha512:71f5a21c44fb133a4778f72ec62fedd5b109b965303a466e67e615e075836f49573df4e32f9e6262f7fc2105a6e1a462f691fd92890df611cab49c338c81bb09"
        },
        "32bit": {
            "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/5.0.10/windowsdesktop-runtime-5.0.10-win-x86.exe#/setup.exe",
            "hash": "sha512:6ab85e0e2e034252abbe1a7642e679d22de44e3c5702519224378c6cdfcecacd2397b0c974ac83bccab6e632178dd001d4e2c3e1ea925c0d8f9b2cb40a9c17ec"
        }
    },
    "pre_download": "Assert-Administrator",
    "installer": {
        "script": [
            "$programname = \"Microsoft Windows Desktop Runtime*\"",
            "$downloadlink = \"https://dotnet.microsoft.com/download/dotnet/5.0\"",
            "if(!(Get-Package -Provider Programs -IncludeWindowsInstaller -Name \"$programname\" -MinimumVersion $version -ErrorAction SilentlyContinue))",
            "{",
            "   &\"$dir\\setup.exe\" /quiet | Out-Null",
            "   if(!(Get-Package -Provider Programs -IncludeWindowsInstaller -Name \"$programname\" -MinimumVersion $version -ErrorAction SilentlyContinue))",
            "   {",
            "       exit 1",
            "   }",
            "}",
            "Remove-Item \"$dir\\setup.exe\" -Force"
        ]
    },
    "checkver": {
        "url": "https://dotnetcli.blob.core.windows.net/dotnet/release-metadata/releases-index.json",
        "regex": "(?s)([\\d.]+)[^\\d]*?(?<sdkversion>[\\d.]+)[^\\d]*?(?:current|lts)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/$version/windowsdesktop-runtime-$version-win-x64.exe#/setup.exe"
            },
            "32bit": {
                "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/$version/windowsdesktop-runtime-$version-win-x86.exe#/setup.exe"
            }
        },
        "hash": {
            "url": "https://dotnetcli.blob.core.windows.net/dotnet/checksums/$version-sha.txt",
            "mode": "extract"
        }
    }
}
