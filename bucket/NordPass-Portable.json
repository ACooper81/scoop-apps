{
    "version": "5.6.11",
    "description": "NordPass. A password management solution for a secure digital life.",
    "homepage": "https://nordpass.com/",
    "license": "Shareware",
    "changelog": "https://nordpass.com/blog/desktop-app-update-notes/",
    "architecture": {
        "64bit": {
            "url": "https://downloads.npass.app/windows/NordPassSetup.exe#/dl.7z",
            "hash": "sha512:ca5c640a0c5cc074c9d042b08ebdca1b4319be234b06ecd56e8a58fca270ad31af0ccff2aa8eb9a97dbb3c943518e040cacd352e0654c40f5616fd646b72c375",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\""
        },
        "32bit": {
            "url": "https://downloads.npass.app/windows_x86/NordPassSetup_x86.exe#/dl.7z",
            "hash": "sha512:fddc23d6ecdaf71ae3a5d8889e48b0d61d4cecbdef13eeb75a5b57bbea14ededa40f9740bea6235eacf7f422c780345235d941ada96168d7f041b488a8f73c07",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-32.7z\" \"$dir\""
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\Uninstall*\", \"$dir\\resources\\app-update.yml\"  -Force -Recurse",
    "bin": [
        [
            "NordPass.exe",
            "NordPass",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "shortcuts": [
        [
            "NordPass.exe",
            "NordPass",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "persist": "User Data",
    "checkver": {
        "url": "https://downloads.npass.app/windows/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.npass.app/windows/NordPassSetup.exe#/dl.7z",
                "hash": {
                    "url": "https://downloads.npass.app/windows/latest.yml",
                    "regex": "sha512:\\s*$base64"
                }
            },
            "32bit": {
                "url": "https://downloads.npass.app/windows_x86/NordPassSetup_x86.exe#/dl.7z",
                "hash": {
                    "url": "https://downloads.npass.app/windows_x86/latest.yml",
                    "regex": "sha512:\\s*$base64"
                }
            }
        }
    }
}
