{
    "version": "5.12.0.8964",
    "description": "Zoom. Video and audio conferencing, chat, and webinars",
    "homepage": "https://zoom.us",
    "license": {
        "identifier": "Proprietary",
        "url": "https://zoom.us/terms"
    },
    "architecture": {
        "64bit": {
            "url": "https://zoom.us/client/5.12.0.8964/ZoomInstallerFull.exe?archType=x64#/dl.7z",
            "hash": "9d938596b9d77decd0ad4e9f9d302a26fd4fd83656294f1dc22ef73a38016048"
        },
        "32bit": {
            "url": "https://zoom.us/client/5.12.0.8964/ZoomInstallerFull.exe#/dl.7z",
            "hash": "b01e2a69500444300f9b2abacae80f111ee4de6444b0c44d0d74cc91e38c4bff"
        }
    },
    "extract_dir": "Zoom\\bin",
    "post_install": [
        "Write-Host \"Registering zoommtg:// protocol handler\"",
        "$regpath = \"HKCU:\\SOFTWARE\\Classes\\zoommtg\"",
        "New-Item -Path $regpath -Value \"Zoom (Application Name)\" -Force | Out-Null",
        "New-ItemProperty -Path $regpath -Name \"URL Protocol\" -PropertyType String -Force | Out-Null",
        "New-Item -Path \"$regpath\\shell\\open\\command\" -Value \"`\"$dir\\Zoom.exe`\" --url=`\"%1`\"\" -Force| Out-Null"
    ],
    "uninstaller": {
        "script": [
            "Write-Host \"Unregistering zoommtg:// protocol handler\"",
            "Remove-Item \"HKCU:\\SOFTWARE\\Classes\\zoommtg\" -Recurse -Force"
        ]
    },
    "bin": "Zoom.exe",
    "shortcuts": [
        [
            "Zoom.exe",
            "Zoom"
        ]
    ],
    "checkver": {
        "url": "https://launcher.zoom.us/upgrade?os=win",
        "regex": "Current-version=([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://zoom.us/client/$version/ZoomInstallerFull.exe?archType=x64#/dl.7z"
            },
            "32bit": {
                "url": "https://zoom.us/client/$version/ZoomInstallerFull.exe#/dl.7z"
            }
        }
    }
}
