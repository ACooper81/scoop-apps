    "hash": "df578d88e38a3f7ff81e27f11db25664d56fb082a5c9588395ec8d06eada7646",
    "installer": {
        "script": [
            "$programname = \"Teamviewer*\"",
            "if(!(Get-Package -Provider Programs -IncludeWindowsInstaller -Name \"$programname\" -RequiredVersion $version -ErrorAction SilentlyContinue))",
            "{",
            "   &\"$dir\\setup.exe\" /S /norestart | Out-Null",
            "   if(!(Get-Package -Provider Programs -IncludeWindowsInstaller -Name \"$programname\" -RequiredVersion $version -ErrorAction SilentlyContinue))",
            "   {",
            "       exit 1",
            "   }",
            "}",
            "Remove-Item \"$dir\\setup.exe\" -Force"
        ]
    },
    "post_install": [
        "$CommonDesktopPath = [Environment]::GetFolderPath(\"CommonDesktopDirectory\")",
        "if (Test-Path \"$CommonDesktopPath\\TeamViewer.lnk\") {Remove-Item \"$CommonDesktopPath\\TeamViewer.lnk\" -Force}"
    ],
    "uninstaller": {
        "script": [
            "$programname = \"Teamviewer*\"",
            "$ProgramFilesPath = [Environment]::GetFolderPath(\"ProgramFilesX86\")",
            "$processname = \"teamviewer.exe\"",
            "if(Get-Package -Provider Programs -IncludeWindowsInstaller -Name \"$programname\" -RequiredVersion $version -ErrorAction SilentlyContinue)",
            "{",
            "   taskkill /F /IM $processname /FI 'status eq running'",
            "   &\"$ProgramFilesPath\\TeamViewer\\uninstall.exe\" /S /norestart | Out-Null",
            "   Start-Sleep -s 10",
            "   if(Get-Package -Provider Programs -IncludeWindowsInstaller -Name \"$programname\" -RequiredVersion $version -ErrorAction SilentlyContinue)",
            "   {",
            "       exit 1",
            "   }",
            "}"
        ]
    },
    "checkver": {
        "url": "https://www.teamviewer.com/en/download/windows/",
        "re": ">\\s*([\\d.]+)\\s*<"
    },
    "autoupdate": {
        "url": "https://dl.teamviewer.com/download/TeamViewer_Setup.exe#/setup.exe"
    }
}
