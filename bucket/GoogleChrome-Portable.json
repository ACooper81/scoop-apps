    "changelog": "https://chromereleases.googleblog.com/",
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/release2/chrome/acosgr5ufcefr7w7nv4v6k4ebdda_117.0.5938.132/117.0.5938.132_chrome_installer.exe#/dl.7z",
            "hash": "e0861cece7b601ace454f1d05d6c5528aefb34441573791c00466ac57b0830d5"
        },
        "32bit": {
